,nom_cid_loja string COMMENT 'nome cidade loja'
,cod_uf_loja char(2) COMMENT 'codigo uf loja'
,num_ddd_loja char(4) COMMENT 'numero ddd loja'
,num_fone_loja int COMMENT 'numero telefone loja'
,nom_geren string COMMENT 'nome gerente'
,nom_region string COMMENT 'nome regional'
,ind_sit char(1) COMMENT 'indicador situacao'
,cod_empr int COMMENT 'codigo empresa'
,cod_tpo_local int COMMENT 'codigo tipo local'
,cod_concl char(2) COMMENT 'codigo conclusao'
,cod_gru_varjo char(3) COMMENT 'codigo grupo varejo'
,tam_area_loja int COMMENT 'tamanho area loja'
,tam_area_vend int COMMENT 'tamanho area venda'
,qtd_check_out int COMMENT 'quantidade check out'
