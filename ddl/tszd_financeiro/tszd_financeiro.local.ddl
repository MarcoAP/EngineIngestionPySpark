,num_cgc_barra_loja int COMMENT 'numero cgc barra loja'
,num_dig_cgc_loja int COMMENT 'numero digito cgc loja'
,dat_sit_local string COMMENT 'data situacao local'
,dat_inaug_local string COMMENT 'data inauguracao local'
,dat_atua_reg string COMMENT 'data atualizacao registro'
,hor_ini_receb string COMMENT 'hora inicial recebimento'
,hor_abert string COMMENT 'hora abertura'
,hor_fecham string COMMENT 'hora fechamento'
,num_seq int COMMENT 'sequencia que o registro aparece no arquivo'
,ind_ativ boolean COMMENT 'indicador ativo'
