,ind_loja_24horas char(1) COMMENT 'indicador loja 24horas'
,ind_tpo_oper char(2) COMMENT 'indicador tipo operacao'
,ind_tpo_loja char(2) COMMENT 'indicador tipo loja'
,cod_concl_negatv char(2) COMMENT 'codigo conclusao negativo'
